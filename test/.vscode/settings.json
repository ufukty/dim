{
  "[json]": {
    "dim.rules": [
      {
        "pattern": ".*\\$.*" // lines like: $schema, $url
      }
    ]
  },
  "[http]": {
    "dim.rules": [
      {
        "pattern": ".*\\$.*"
      },
      {
        "pattern": "###",
        "opacity": "min"
      }
    ]
  },
  "[go]": {
    "dim.rules": [
      {
        "pattern": "if\\s+err != nil\\s*{(?:(?:[^{}]|\\n)*{(?:[^}]|\\n)*})?(?:[^}]|\\n)*}",
        "flags": "gs",
        "opacity": "min"
      },
    ]
  },
  "[js]": {
    "dim.rules": [
      {
        "pattern": "announce\\(.*?\\);?",
      },
      {
        "pattern": "logger.verbose\\([^\\n]*\\);?",
      },
      {
        "pattern": "logger",
      },
    ]
  },
  "dim.rules": [
    {
      "pattern": "//[^\\n]*", // comment lines
      "opacity": "max"
    }
  ],
  "dim.defaultopacity": "mid",
  "dim.defaultScanLimit": 500,
  "dim.defaultMaxLinesBetween": 5,
  "dim.valueForMinTier": 0.2,
  "dim.valueForMidTier": 0.3,
  "dim.valueForMaxTier": 0.4
}